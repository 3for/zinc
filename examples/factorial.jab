/*
    Algorithm: Factorial
*/

inputs {}

let mut fact: field = 1;

for i in 2..6 {
    fact = fact * i as field;
};

require(fact == 120 as field);

type Huylo = (field,);

struct Putin {
    huylo: field,
    a: bool,
    b: (),
    c: Huylo,
};

let value: Putin = Putin {
    huylo: 0 as field,
    a: true,
    b: (),
    c: (1,),
};
