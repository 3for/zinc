/*
    The fibonacci algorithm test
*/

inputs {
    dummy: field;
}

let mut value_1: field = 0;
let mut value_2: field = 1;
let mut fibo = value_1;
debug(fibo);

for i in 0..42 {
    fibo = value_1 + value_2;
    value_1 = value_2;
    value_2 = fibo;
    debug(fibo);
};

require(fibo == 433494437 as field);
