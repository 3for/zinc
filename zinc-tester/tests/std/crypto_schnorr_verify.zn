struct Signature {
    r: (field, field),
    s: field,
    pk: (field, field),
}

const MESSAGE_LENGTH: u64 = 8;

fn main(signature: Signature, message: [bool; MESSAGE_LENGTH]) -> bool {
    std::crypto::schnorr::verify(
        signature.r.0,
        signature.r.1,
        signature.s,
        signature.pk.0,
        signature.pk.1,
        message
    )
}
