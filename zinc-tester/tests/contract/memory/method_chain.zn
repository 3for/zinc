contract Test {
    a: u248;
    b: u248;

    pub fn default(mut self, value: u248) -> u248 {
        self.a = 5 as u248;
        self.b = 11 as u248;

        self.a + self.quadruple(self.triple(self.double(value))) + self.b
    }

    fn double(self, value: u248) -> u248 {
        value * 2
    }

    fn triple(self, value: u248) -> u248 {
        3 * value
    }

    fn quadruple(self, value: u248) -> u248 {
        value * 4
    }
}
