struct Signature {
    r: (field, field),
    s: field,
    pk: (field, field),
}

fn main(signature: Signature, message: field) -> bool {
    std::crypto::schnorr::verify(
        signature.r.0,
        signature.r.1,
        signature.s,
        signature.pk.0,
        signature.pk.1,
        message
    )
}
