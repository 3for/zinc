use std::convert;
use std::array::truncate;
use std::crypto::sha256;

fn main(preimage: u248) -> u248 {
    convert::from_bits_unsigned(
        truncate(
            sha256(
                convert::to_bits(preimage)
            ), 248
        )
    )
}
