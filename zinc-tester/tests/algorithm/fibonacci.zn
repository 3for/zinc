const INDEX: u8 = 16;

fn main(n: u8) -> field {
    let mut value_1: field = 0;
    let mut value_2: field = 1;
    let mut fibo = value_1;

    for i in 1..=INDEX while i + 2 <= n {
        fibo = value_1 + value_2;
        value_1 = value_2;
        value_2 = fibo;
    }

    fibo
}
