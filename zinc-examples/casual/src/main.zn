struct Data {
    a: u8,
    b: u8,
}

impl Data {
    pub fn double(mut self) -> Self {
        self.a *= 2;
        self.b *= 2;
        self
    }
}

fn main() {
    let mut data = Data { a: 2, b: 1 };
    dbg!("Data x1 is: {}", data);

    let data_x2 = data.double();
    dbg!("Data x2 is: {}", data_x2);
}
