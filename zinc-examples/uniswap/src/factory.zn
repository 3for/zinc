//!
//! The 'Uniswap' contract factory.
//!

type Address = u160;

struct Factory {
    address: Address,
}

impl Factory {
    pub fn from_address(address: Address) -> Self {
        Self {
            address: address
        }
    }

    pub fn get_exchange(self, token_addr: Address) -> Address {
        self.address
    }
}
