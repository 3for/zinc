//!
//! The '-' operator tests.
//!

struct ResultU8 {
    sub_zero_zero: u8,
    sub_value_zero: u8,
    sub_value_value: u8,
    sub_max: u8,
}

impl ResultU8 {
    fn test() -> Self {
        Self {
            sub_zero_zero: 0 - 0,
            sub_value_zero: 5 - 0,
            sub_value_value: 5 - 3,
            sub_max: 255 - 0,
        }
    }
}

struct ResultI8 {
    sub_zero_zero: i8,
    sub_value_zero: i8,
    sub_value_value: i8,
    sub_max: i8,
    sub_min: i8,
}

impl ResultI8 {
    fn test() -> Self {
        Self {
            sub_zero_zero: 0 as i8 - 0 as i8,
            sub_value_zero: 13 as i8 - 0 as i8,
            sub_value_value: 13 as i8 - 8 as i8,
            sub_max: 127 as i8 - 0 as i8,
            sub_min: -128 - 0 as i8,
        }
    }
}

