//!
//! The '*' operator tests.
//!

struct ResultU8 {
    mul_zero_zero: u8,
    mul_zero_value: u8,
    mul_value_zero: u8,
    mul_value_value: u8,
    mul_max: u8,
}

impl ResultU8 {
    fn test() -> Self {
        Self {
            mul_zero_zero: 0 * 0,
            mul_zero_value: 0 * 5,
            mul_value_zero: 5 * 0,
            mul_value_value: 5 * 3,
            mul_max: 255 * 1,
        }
    }
}

struct ResultI8 {
    mul_zero_zero: i8,
    mul_zero_value: i8,
    mul_value_zero: i8,
    mul_value_value: i8,
    mul_max: i8,
    mul_min: i8,
}

impl ResultI8 {
    fn test() -> Self {
        Self {
            mul_zero_zero: 0 as i8 * 0 as i8,
            mul_zero_value: 0 as i8 * 13 as i8,
            mul_value_zero: 13 as i8 * 0 as i8,
            mul_value_value: 13 as i8 * 8 as i8,
            mul_max: 127 as i8 * 1 as i8,
            mul_min: -128 * 1 as i8,
        }
    }
}

