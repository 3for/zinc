//!
//! The '!' operator tests.
//!

struct ResultBool {
    or_false: bool,
    or_true: bool,
}

impl ResultBool {
    fn test() -> Self {
        Self {
            or_false: !false,
            or_true: !true,
        }
    }
}

struct ResultWitness {
    or_const: bool,
    or_value: bool,
}

impl ResultWitness {
    fn test(witness: bool) -> Self {
        Self {
            or_const: !false,
            or_value: !witness,
        }
    }
}

struct Result {
    result_bool: ResultBool,
    result_witness: ResultWitness,
}

impl Result {
    fn test(witness: bool) -> Self {
        Self {
            result_bool: ResultBool::test(),
            result_witness: ResultWitness::test(witness),
        }
    }
}
