//!
//! The unary '-' operator tests.
//!

struct Result8 {
    sub_zero: i8,
    sub_value: i8,
    sub_min: i8,
}

impl Result8 {
    fn test() -> Self {
        Self {
            sub_zero: -0,
            sub_value: -5,
            sub_min: -128,
        }
    }
}

struct Result64 {
    sub_zero: i64,
    sub_value: i64,
    sub_min: i64,
}

impl Result64 {
    fn test() -> Self {
        Self {
            sub_zero: -(0 as u64),
            sub_value: -(34 as u64),
            sub_min: -0x7FFF_FFFF_FFFF_FFFF,
        }
    }
}

struct Result248 {
    sub_zero: i248,
    sub_value: i248,
    sub_min: i248,
}

impl Result248 {
    fn test() -> Self {
        Self {
            sub_zero: -(0 as u248),
            sub_value: -(233 as u248),
            sub_min: -0x7F_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF_FFFF,
        }
    }
}

struct ResultWitness {
    sub_const: i8,
    sub_value: i8,
}

impl ResultWitness {
    fn test(witness: u8) -> Self {
        Self {
            sub_const: -42,
            sub_value: -witness,
        }
    }
}

struct Result {
    result_u8: Result8,
    result_u64: Result64,
    result_u248: Result248,
    result_witness: ResultWitness,
}

impl Result {
    fn test(witness: u8) -> Self {
        Self {
            result_u8: Result8::test(),
            result_u64: Result64::test(),
            result_u248: Result248::test(),
            result_witness: ResultWitness::test(witness),
        }
    }
}
