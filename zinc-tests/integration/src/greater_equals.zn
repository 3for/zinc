    greater_equals_value_zero: bool,
    greater_equals_value_value: bool,
    greater_equals_max: bool,
}

impl ResultU64 {
    fn test() -> Self {
        Self {
            greater_equals_zero_zero: 0 as u64 >= 0 as u64,
            greater_equals_zero_value: 0 as u64 >= 34 as u64,
            greater_equals_value_zero: 34 as u64 >= 0 as u64,
            greater_equals_value_value: 34 as u64 >= 34 as u64,
            greater_equals_max: 0xFFFF_FFFF_FFFF_FFFF as u64 >= 0xFFFF_FFFF_FFFF_FFFF as u64,
        }
    }
}

struct ResultI64 {
    greater_equals_zero_zero: bool,
    greater_equals_zero_value: bool,
    greater_equals_value_zero: bool,
    greater_equals_value_value: bool,
    greater_equals_max: bool,
    greater_equals_min: bool,
}

impl ResultI64 {
    fn test() -> Self {
        Self {
            greater_equals_zero_zero: 0 as i64 >= 0 as i64,
            greater_equals_zero_value: 0 as i64 >= 89 as i64,
            greater_equals_value_zero: 89 as i64 >= 0 as i64,
            greater_equals_value_value: 89 as i64 >= 89 as i64,
            greater_equals_max: 0x7FFF_FFFF_FFFF_FFFF as i64 >= 0x7FFF_FFFF_FFFF_FFFF as i64,
            greater_equals_min: -0x8000_0000_0000_0000 >= -0x8000_0000_0000_0000,
        }
    }
}

struct ResultU248 {
    greater_equals_zero_zero: bool,
    greater_equals_zero_value: bool,
