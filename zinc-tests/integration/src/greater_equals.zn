    greater_equals_zero_value: bool,
    greater_equals_value_zero: bool,
    greater_equals_value_value: bool,
    greater_equals_max: bool,
    greater_equals_min: bool,
}

impl ResultI248 {
    fn test() -> Self {
        Self {
            greater_equals_zero_zero: 0 as i248 >= 0 as i248,
            greater_equals_zero_value: 0 as i248 >= 233 as i248,
            greater_equals_value_zero: 233 as i248 >= 0 as i248,
            greater_equals_value_value: 233 as i248 >= 233 as i248,
