//!
//! The '>=' operator tests.
//!

struct ResultU8 {
    greater_equals_zero_zero: bool,
    greater_equals_zero_value: bool,
    greater_equals_value_zero: bool,
    greater_equals_value_value: bool,
    greater_equals_max: bool,
}

impl ResultU8 {
    fn test() -> Self {
        Self {
            greater_equals_zero_zero: 0 >= 0,
            greater_equals_zero_value: 0 >= 5,
            greater_equals_value_zero: 5 >= 0,
            greater_equals_value_value: 5 >= 5,
            greater_equals_max: 255 >= 255,
        }
    }
}

struct ResultI8 {
    greater_equals_zero_zero: bool,
    greater_equals_zero_value: bool,
    greater_equals_value_zero: bool,
    greater_equals_value_value: bool,
    greater_equals_max: bool,
    greater_equals_min: bool,
}

impl ResultI8 {
    fn test() -> Self {
        Self {
            greater_equals_zero_zero: 0 as i8 >= 0 as i8,
            greater_equals_zero_value: 0 as i8 >= 13 as i8,
            greater_equals_value_zero: 13 as i8 >= 0 as i8,
            greater_equals_value_value: 13 as i8 >= 13 as i8,
            greater_equals_max: 127 >= 127,
            greater_equals_min: -128 >= -128,
        }
    }
}

